            {{{body}}}


            {{#if appJavaScripts}}
                {{#appJavaScripts}}
                    {{{javascriptOutputHelper this ../this  }}}
                {{/appJavaScripts}}
            {{/if}}

            {{#if templates}}
            <script id="templates_{{_folderName}}">
                (function () {
                    var template  = Handlebars.template,
                            templates = Handlebars.templates = Handlebars.templates || {};

                    {{#templates}}
                        templates['{{{prefix}}}{{{name}}}'] = template({{{template}}});
                    {{/templates}}
                }());
            </script>
            {{/if}}
